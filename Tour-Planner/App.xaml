<Application x:Class="Tour_Planner.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" 
             xmlns:local="clr-namespace:Tour_Planner" 
             xmlns:Converters="clr-namespace:Tour_Planner.Converters;assembly=Tour-Planner.Converters"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <local:Container x:Key="Container"/>
            <Converters:NullToVisibilityConverter x:Key="NullToVisibilityConverter"/>
            <Converters:EnumToStringConverter x:Key="DefaultEnumToStringConverter"/>
            <BitmapImage x:Key="DefaultImage" UriSource="Images/tour.png"/>
        </ResourceDictionary>
    </Application.Resources>
</Application>
